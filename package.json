{
    "name": "overrustle",
    "version": "1.0.0",
    "description": "OverRustle",
    "private": true,
    "engines": {
        "node": ">=12"
    },
    "scripts": {
        "dev": "vite",
        "vite:build": "vite build",
        "clean": "rm -rf public/assets public/index.html",
        "build": "npm run clean && npm run build:bundle",
        "build:bundle": "webpack --progress",
        "build:production": "npm run --prod build",
        "dev-server": "npm run build && webpack-dev-server --inline --hot",
        "flow": "flow check",
        "lint": "eslint webpack.config.js --ext .js --ext .jsx src"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.30",
        "@fortawesome/free-brands-svg-icons": "^5.11.2",
        "@fortawesome/free-solid-svg-icons": "^5.11.2",
        "@fortawesome/react-fontawesome": "^0.1.7",
        "bluebird": "3.7.2",
        "bootstrap": "^4.3.1",
        "browser-cookies": "^1.1.0",
        "clappr": "0.3.11",
        "classnames": "^2.2.5",
        "dotenv": "8.2.0",
        "glyphicons-halflings": "^1.9.1",
        "history": "^5.0.0",
        "idx": "^2.5.6",
        "isomorphic-fetch": "^2.2.1",
        "level-selector": "^0.2.0",
        "lodash": "^4.17.20",
        "prop-types": "^15.7.2",
        "qs": "6.9.1",
        "react": "^18.2.0",
        "react-bootstrap": "1.3.0",
        "react-custom-scrollbars": "^4.2.1",
        "react-dom": "^18.2.0",
        "react-draggable": "^4.4.5",
        "react-idle-timer": "^4.0.9",
        "react-loadable": "^5.3.1",
        "react-redux": "^7.2.1",
        "react-router-dom": "^4.1.1",
        "react-use": "15.3.3",
        "recompose": "^0.30.0",
        "reconnecting-websocket": "^4.4.0",
        "redux": "4.0.5",
        "redux-logger": "^3.0.1",
        "redux-thunk": "^2.1.0"
    },
    "devDependencies": {
        "@types/react": "^18.0.25",
        "@types/react-dom": "^18.0.9",
        "@types/react-router-dom": "^5.3.3",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "@vitejs/plugin-react": "^2.2.0",
        "eslint": "7.7.0",
        "eslint-plugin-react": "7.20.6",
        "sass": "^1.56.1",
        "vite": "^3.2.4"
    },
    "prettier": {
        "tabWidth": 4,
        "trailingComma": "es5",
        "semi": true,
        "singleQuote": true,
        "printWidth": 120,
        "arrowParens": "avoid"
    }
}
